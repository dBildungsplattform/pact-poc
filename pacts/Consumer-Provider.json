{
  "consumer": {
    "name": "Consumer"
  },
  "interactions": [
    {
      "description": "a request for a user with specific id",
      "providerState": "I have a list of users",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/user",
        "query": "id=1"
      },
      "response": {
        "body": {
          "id": 1,
          "name": "Max",
          "password": "secret"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.1.0"
    },
    "pactRust": {
      "ffi": "0.4.9",
      "models": "1.1.11"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "Provider"
  }
}